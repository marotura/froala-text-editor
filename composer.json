{
    "name": "marotura/froala-text-editor",
    "description": "A custom Laravel Nova field using Froala editor.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Maronova\\FroalaTextEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maronova\\FroalaTextEditor\\FieldServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Marotura B.V.",
            "email": "techniek@marotura.com"
        },
        {
            "name": "Arminda R. Marin",
            "email": "a.marin@marotura.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
